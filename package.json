{
  "name": "devs-llc-angular-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:watchAll": "npx jest --watchAll",
    "test:coverage": "npx jest --coverage",
    "analyze": "source-map-explorer dist/devs-llc-angular-project/*.js"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@tanstack/angular-query-experimental": "^5.90.2",
    "date-fns": "^4.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^20.0.0",
    "@angular/build": "^20.3.1",
    "@angular/cli": "^20.3.1",
    "@angular/compiler-cli": "^20.3.0",
    "@faker-js/faker": "^10.0.0",
    "@ngneat/spectator": "^21.0.1",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-preset-angular": "^14.6.1",
    "jsdom": "^25.0.0",
    "source-map-explorer": "^2.5.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  }
}
